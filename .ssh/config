Host github.com
  IgnoreUnknown AddKeysToAgent,UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  Preferredauthentications publickey
  IdentityFile ~/.ssh/github_zerospiel_rsa

# Private GitLab instance
Host some.host.name.com
  IgnoreUnknown AddKeysToAgent,UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  Preferredauthentications publickey
	IdentityFile ~/.ssh/gitlab_mmorgoev_rsa
